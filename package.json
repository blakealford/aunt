{
  "name": "aussiebb_aunt",
  "version": "1.0.0",
  "description": "Display your monthly usage and data left.",
  "main": "src/main.js",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "publish": "electron-forge publish",
    "make": "electron-forge make --skip-package",
    "install": "electron-rebuild --pre-gyp-fix"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lukealford/aussiebb_tray"
  },
  "author": "Luke Alford",
  "license": "MIT",
  "dependencies": {
    "electron-compile": "^6.4.2",
    "electron-context-menu": "^0.9.1",
    "electron-squirrel-startup": "^1.0.0",
    "electron-store": "^1.3.0",
    "electron-unhandled": "^1.1.0",
    "electron-updater": "^2.21.11",
    "handlebars": "^4.1.1",
    "handlebars-intl": "^1.1.2",
    "keytar": "^4.2.1",
    "moment": "^2.22.1",
    "request": "^2.86.0"
  },
  "devDependencies": {
    "electron-forge": "^5.2.4",
    "electron-prebuilt-compile": "2.0.0"
  },
  "config": {
    "forge": {
      "make_targets": {
        "win32": [
          "squirrel"
        ],
        "darwin": [
          "zip"
        ],
        "linux": [
          "deb",
          "rpm"
        ]
      },
      "electronPackagerConfig": {
        "packageManager": "npm",
        "asar": true,
        "quiet": true,
        "icon": "src/icons/aussie_icon",
        "version-string.CompanyName": "CE",
        "version-string.FileDescription": "CE",
        "version-string.ProductName": "AussieBB AUNT"
      },
      "electronWinstallerConfig": {

      },
      "electronInstallerDMG": {},
      "electronInstallerFlatpak": {},
      "electronInstallerDebian": {},
      "electronInstallerRedhat": {},
      "github_repository": {
        "owner": "lukealford",
        "name": "aussiebb_tray"
      }
    }
  }
}
